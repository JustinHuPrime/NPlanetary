{
  "name": "nplanetary",
  "version": "0.1.8",
  "description": "A game of logistics and tactics in the future solar system of the seventies",
  "scripts": {
    "cover": "nyc --reporter text --reporter html npm run test",
    "build": "npm run build-client && npm run build-server",
    "build-client": "tsc -p tsconfig.client.json",
    "build-server": "tsc -p tsconfig.server.json",
    "test": "mocha --require ts-node/register --timeout 10000 ./test/**/*.spec.ts",
    "pretty": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf compiled public_html/scripts",
    "start": "node compiled/server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustinHuPrime/NPlanetary.git"
  },
  "keywords": [
    "template"
  ],
  "author": "Justin Hu <justin.hu@alumni.ubc.ca>",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/JustinHuPrime/NPlanetary/issues"
  },
  "homepage": "https://github.com/JustinHuPrime/NPlanetary#readme",
  "engines": {
    "node": ">= 18 < 19"
  },
  "dependencies": {
    "restify": "^10.0.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/restify": "^8.5.5",
    "@types/ws": "^8.5.4",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
